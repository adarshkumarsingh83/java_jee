<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<properties>
    <comment>database properties</comment>
    <entry key="dataSource.driverClass">oracle.jdbc.driver.OracleDriver</entry>
    <entry key="dataSource.url">jdbc:oracle:thin:@</entry>
    <entry key="dataSource.hostname">127.0.0.1</entry>
    <entry key="dataSource.portNumber">:1521</entry>
    <entry key="dataSource.database">:XE</entry>
    <entry key="dataSource.username">system</entry>
    <entry key="dataSource.password">admin</entry>

    <entry key="sql.SelectEmployee">{call getEmpProcedure(?,?,?)}</entry>
    <entry key="sql.SelectAllEmployee">{call getAllEmpProcedure(?,?)}</entry>
    <entry key="sql.InsertEmployee">{call insertEmpProcedure(?,?,?,?)}</entry>
    <entry key="sql.UpdateEmployee">{call updateEmpProcedure(?,?,?,?)}</entry>
    <entry key="sql.DeleteEmployee">{call deleteEmpProcedure(?,?)}</entry>
</properties>